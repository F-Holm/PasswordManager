{
    "version": 4,
    "configurePresets": [
        {
            "name": "std-23",
            "hidden": true,
            "cacheVariables": {
                "CMAKE_C_STANDARD": "23",
                "CMAKE_C_STANDARD_REQUIRED": "ON",
                "CMAKE_CXX_STANDARD": "23",
                "CMAKE_CXX_STANDARD_REQUIRED": "ON"
            }
        },
        {
            "name": "std",
            "hidden": true,
            "cacheVariables": {
                "CMAKE_C_STANDARD_REQUIRED": true,
                "CMAKE_CXX_STANDARD_REQUIRED": true
            }
        },
        {
            "name": "no-std",
            "hidden": true,
            "cacheVariables": {
                "CMAKE_C_STANDARD_REQUIRED": false,
                "CMAKE_CXX_STANDARD_REQUIRED": false
            }
        },
        {
            "name": "ccache",
            "hidden": true,
            "cacheVariables": {
                "CMAKE_C_COMPILER_LAUNCHER": "ccache",
                "CMAKE_CXX_COMPILER_LAUNCHER": "ccache"
            }
        },
        {
            "name": "debug",
            "hidden": true,
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Debug"
            }
        },
        {
            "name": "release",
            "hidden": true,
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Release"
            }
        }
    ]
}