{
    "version": 10,
    "configurePresets": [
        {
            "name": "clang",
            "hidden": true,
            "cacheVariables": {
                "CMAKE_C_COMPILER": "clang",
                "CMAKE_CXX_COMPILER": "clang++"
            }
        },
        {
            "name": "gcc",
            "hidden": true
        },
        {
            "name": "cross-compile-linux",
            "hidden": true,
            "cacheVariables": {
                "CMAKE_FIND_ROOT_PATH_MODE_LIBRARY": "NEVER",
                "CMAKE_FIND_ROOT_PATH_MODE_PROGRAM": "ONLY",
                "CMAKE_FIND_ROOT_PATH_MODE_INCLUDE": "ONLY"
            }
        },
        {
            "name": "mingw-linux",
            "hidden": true,
            "inherits": [
                "cross-compile-linux"
            ]
        },
        {
            "name": "mingw-clang-linux",
            "hidden": true,
            "inherits": [
                "cross-compile-linux",
                "clang"
            ]
        },
        {
            "name": "mingw-gcc-linux",
            "hidden": true,
            "inherits": [
                "cross-compile-linux",
                "gcc"
            ]
        },
        {
            "name": "mingw-gcc-linux-x64",
            "hidden": true,
            "inherits": "mingw-gcc-linux",
            "cacheVariables": {
                "CMAKE_C_COMPILER": "x86_64-w64-mingw32-gcc",
                "CMAKE_CXX_COMPILER": "x86_64-w64-mingw32-g++"
            }
        },
        {
            "name": "mingw-gcc-linux-x86",
            "hidden": true,
            "inherits": "mingw-gcc-linux",
            "cacheVariables": {
                "CMAKE_C_COMPILER": "i686-w64-mingw32-gcc",
                "CMAKE_CXX_COMPILER": "i686-w64-mingw32-g++"
            }
        },
        {
            "name": "mingw-gcc-linux-arm64",
            "hidden": true,
            "inherits": "mingw-gcc-linux",
            "cacheVariables": {
                "CMAKE_C_COMPILER": "aarch64-w64-mingw32-gcc",
                "CMAKE_CXX_COMPILER": "aarch64-w64-mingw32-g++"
            }
        },
        {
            "name": "mingw-gcc-linux-arm",
            "hidden": true,
            "inherits": "mingw-gcc-linux",
            "cacheVariables": {
                "CMAKE_C_COMPILER": "armv7-w64-mingw32-gcc",
                "CMAKE_CXX_COMPILER": "armv7-w64-mingw32-g++"
            }
        },
        {
            "name": "mingw-clang-linux-x64",
            "hidden": true,
            "inherits": [
                "mingw-clang-linux"
            ],
            "cacheVariables": {
                "CMAKE_C_FLAGS": "--target=x86_64-w64-mingw32",
                "CMAKE_CXX_FLAGS": "--target=x86_64-w64-mingw32"
            }
        },
        {
            "name": "mingw-clang-linux-x86",
            "hidden": true,
            "inherits": [
                "mingw-clang-linux"
            ],
            "cacheVariables": {
                "CMAKE_C_FLAGS": "--target=i686-w64-mingw32",
                "CMAKE_CXX_FLAGS": "--target=i686-w64-mingw32"
            }
        },
        {
            "name": "mingw-clang-linux-arm64",
            "hidden": true,
            "inherits": [
                "mingw-clang-linux"
            ],
            "cacheVariables": {
                "CMAKE_C_FLAGS": "--target=aarch64-w64-mingw32",
                "CMAKE_CXX_FLAGS": "--target=aarch64-w64-mingw32"
            }
        },
        {
            "name": "mingw-clang-linux-arm",
            "hidden": true,
            "inherits": [
                "mingw-clang-linux"
            ],
            "cacheVariables": {
                "CMAKE_C_FLAGS": "--target=armv7-w64-mingw32",
                "CMAKE_CXX_FLAGS": "--target=armv7-w64-mingw32"
            }
        },
        {
            "name": "clang-linux-x64",
            "hidden": true,
            "inherits": "clang"
        },
        {
            "name": "clang-linux-x86",
            "hidden": true,
            "inherits": "clang",
            "cacheVariables": {
                "CMAKE_C_FLAGS": "--target=i686-linux-gnu",
                "CMAKE_CXX_FLAGS": "--target=i686-linux-gnu"
            }
        },
        {
            "name": "clang-linux-arm64",
            "hidden": true,
            "inherits": "clang",
            "cacheVariables": {
                "CMAKE_C_FLAGS": "--target=aarch64-linux-gnu",
                "CMAKE_CXX_FLAGS": "--target=aarch64-linux-gnu"
            }
        },
        {
            "name": "clang-linux-arm",
            "hidden": true,
            "inherits": "clang",
            "cacheVariables": {
                "CMAKE_C_FLAGS": "--target=arm-linux-gnueabihf",
                "CMAKE_CXX_FLAGS": "--target=arm-linux-gnueabihf"
            }
        },
        {
            "name": "gcc-linux-x64",
            "hidden": true,
            "inherits": "gcc",
            "cacheVariables": {
                "CMAKE_C_COMPILER": "gcc",
                "CMAKE_CXX_COMPILER": "g++"
            }
        },
        {
            "name": "gcc-linux-x86",
            "hidden": true,
            "inherits": "gcc",
            "cacheVariables": {
                "CMAKE_C_COMPILER": "i686-linux-gnu-gcc",
                "CMAKE_CXX_COMPILER": "i686-linux-gnu-g++"
            }
        },
        {
            "name": "gcc-linux-arm64",
            "hidden": true,
            "inherits": "gcc",
            "cacheVariables": {
                "CMAKE_C_COMPILER": "aarch64-linux-gnu-gcc",
                "CMAKE_CXX_COMPILER": "aarch64-linux-gnu-g++"
            }
        },
        {
            "name": "gcc-linux-arm",
            "hidden": true,
            "inherits": "gcc",
            "cacheVariables": {
                "CMAKE_C_COMPILER": "arm-linux-gnueabihf-gcc",
                "CMAKE_CXX_COMPILER": "arm-linux-gnueabihf-g++"
            }
        }
    ]
}