{
    "version": 8,
    "configurePresets": [
        {
            "name": "compilador-default",
            "hidden": true
        },
        {
            "name": "gcc",
            "hidden": true,
            "inherits": [
                "compilador-default"
            ],
            "cacheVariables": {
                "CMAKE_C_COMPILER": "gcc",
                "CMAKE_CXX_COMPILER": "g++"
            }
        },
        {
            "name": "clang",
            "hidden": true,
            "inherits": [
                "compilador-default"
            ],
            "cacheVariables": {
                "CMAKE_C_COMPILER": "clang",
                "CMAKE_CXX_COMPILER": "clang++"
            }
        }
    ]
}